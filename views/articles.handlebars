<h3>News Articles</h3>

<div class="row">
  {{#each articles}}
    <div class="col s12 m6 l4 xl3 flip-card">
      <div class="card">
      <a href="{{link}}" target="_blank">
        <div class="card-image">
          <img src="{{src}}">
          {{!-- <span class="card-title"></span> --}}
        </div>
        <div class="card-content">
          <p> {{position}}. {{title}} </p>
        </div> </a>
        <div class="card-action">
          <a href="#" class="allNotes">All Notes</a>
          <a href="#" class="addNotes">Add Note</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>